					"script": {
						"exec": [
							"const schema = {",
							"    \"type\": \"object\",",
							"    \"properties\": {",
							"        \"event_data\": { ",
							"            \"type\": \"object\",",
							"            \"properties\": {",
							"                \"level\": { \"type\": \"number\" }",
							"            },",
							"            \"required\": [\"level\"]",
							"        }",
							"    },",
							"    \"required\": [\"event_data\"]",
							"};",
							"",
							"pm.test(\"Status code is 200\", function () {",
							"    pm.response.to.have.status(200);",
							"});",
							"",
							"pm.test(\"Response has empty elements\", function () {",
							"    pm.expect(pm.response.json().elements.length).to.eql(0)",
							"});",
